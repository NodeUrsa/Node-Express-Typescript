{
	"name"					: "hydration",
	"cwd"					: "/var/web-hydration",
	"script"				: "/var/web-hydration/app/index.js",
	"error_file"			: "/var/log/web-hydration/error.log",
	"out_file"				: "/var/log/web-hydration/out.log",
	"instances"				: 1,
	"min_uptime"			: "200s",
	"max_restarts"			: 4,
	"max_memory_restart"	: "300M",
	"watch"					: false,
	"merge_logs"			: false,
	"exec_interpreter"		: "node",
	"exec_mode"				: "fork",
	"autorestart"			: true,
	"env"					: {
		"NODE_ENV": "production"
	}
}
